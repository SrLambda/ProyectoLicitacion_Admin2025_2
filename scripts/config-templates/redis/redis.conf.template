bind 0.0.0.0
protected-mode yes
port 6379

requirepass __REDIS_PASSWORD__
masterauth __REDIS_PASSWORD__

appendonly yes
appendfilename "appendonly.aof"
appendfsync everysec

maxmemory __MAX_MEMORY__
maxmemory-policy allkeys-lru

save 900 1
save 300 10
save 60 10000

dir /data

loglevel notice
logfile ""

databases __DATABASES__

timeout 300
tcp-keepalive 300
